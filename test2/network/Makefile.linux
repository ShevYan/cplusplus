#
# Configuration: Debug
#
ifeq "$(CFG)" "Debug"
OUTDIR=Debug
OUTFILE=$(OUTDIR)/libficsnet.so
CFG_INC=-I../include_com 
CFG_LIB=
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/channel.o $(OUTDIR)/module_reg.o \
	$(OUTDIR)/msgsite.o $(OUTDIR)/netconfig.o $(OUTDIR)/netmsg.o \
	$(OUTDIR)/socketpool.o $(OUTDIR)/tcpio.o $(OUTDIR)/tcpserver.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/channel.o $(OUTDIR)/module_reg.o \
	$(OUTDIR)/msgsite.o $(OUTDIR)/netconfig.o $(OUTDIR)/netmsg.o \
	$(OUTDIR)/socketpool.o $(OUTDIR)/tcpio.o $(OUTDIR)/tcpserver.o 

COMPILE=g++ -c  "-DFI_LINUX" "-D_POSIX_" -g -Wreturn-type -fPIC -o "$(OUTDIR)/$(*F).o" $(CFG_INC) $<
LINK=g++  -g -shared -fPIC -o "$(OUTFILE)" $(ALL_OBJ)

# Pattern rules
$(OUTDIR)/%.o : %.cpp
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)
	-cp ./Debug/libficsnet.so ../../bin
	-cp ./Debug/libficsnet.so /usr/local/lib

$(OUTDIR):
	$(MKDIR) -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	$(RM) -f $(OUTFILE)
	$(RM) -f $(OBJ)

# Clean this project and all dependencies
cleanall: clean
endif

