OUTFILE=$(OUTDIR)/testtcpio
CFG_INC=-I../../include_com -I../../network 
CFG_LIB=-lpthread -lcomutility -lrt -lficsnet 
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/testtcpio.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/testtcpio.o -lpthread -lcomutility -lrt -lficsnet 

COMPILE=g++ -c   "-D_POSIX_" "-DFI_LINUX" -g -o "$(OUTDIR)/$(*F).o" $(CFG_INC) "$<"
LINK=g++  -g -o "$(OUTFILE)" $(OBJ) $(CFG_LIB)

# Pattern rules
$(OUTDIR)/%.o : %.cpp
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)
	-cp ./Debug/testtcpio ../../../bin

$(OUTDIR):
	$(MKDIR) -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	$(RM) -f $(OUTFILE)
	$(RM) -f $(OBJ)

# Clean this project and all dependencies
cleanall: clean
